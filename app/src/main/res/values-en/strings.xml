    </string>
    <string name="aplikasi_ini_hanya_dapat_digunakan_sesuai_dengan_ketentuan_yang_berlaku_dan_tidak_untuk_tujuan_yang_melanggar_hukum_n_setiap_bentuk_penyalahgunaan_aplikasi_akan_mengakibatkan_penghentian_akses_secara_sepihak">
